{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Default": "mongodb://localhost:27017/Tharga{environment}_HostSample{part}",
    "Other": "mongodb://localhost:27017/Tharga{environment}_HostSample_Other{part}"
  },
  "MongoDb": {
    "Default": {
      "AccessInfo": {
        "PublicKey": "PUBLIC_KEY",
        "PrivateKey": "PRIVATE_KEY",
        "ClusterId": "CLUSTER_ID"
      },
      "ResultLimit": 100,
      "AutoClean": true,
      "CleanOnStartup": true,
      "DropEmptyCollections": true
    },
    "Other": {
      "ResultLimit": 200
    },
    "ResultLimit": 1000,
    "AutoClean": false,
    "CleanOnStartup": false,
    "DropEmptyCollections": false
  }
}
